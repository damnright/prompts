{
  "mcpServers": {
    "figma-remote-mcp-server": {
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.figma.com/mcp",
        "--static-oauth-client-metadata",
        "{\"client_name\":\"Antigravity\",\"client_uri\":\"https://antigravity.google/\"}"
      ],
      "command": "npx",
      "disabled": false,
      "env": {}
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "env": {}
    },
    "google-maps-platform-code-assist": {
      "command": "npx",
      "args": [
        "-y",
        "@googlemaps/code-assist-mcp@latest"
      ],
      "env": {
        "SOURCE": "antigravity"
      }
    }
  }
}